<h1>Edit My Account</h1>

<% form_for @user, :url => account_path do |f| %>

  <%= render :partial => "layouts/error_list", :collection => @user.errors.full_messages %>

  <%= render :partial => "form_edit", :object => f %>
  <%= f.submit "Update" %>
<% end %>

